    <%!String str;%>
	<%request.setCharacterEncoding("EUC-KR");
	//요청된 값이 한글일 때 세팅하는 코드%>
    <%
	String name = request.getParameter("name");
	//request 는 내장 객체; 임폴트 필요X 
	//getParameter HTML의 객체 불러오기
	String food = request.getParameter("food");

	if (food.equals("ramen")) {
		str = "라면";
	} else if (food.equals("chick")) {
		str = "치킨";
	}else str="피자";
	%>
	좋아하는 음식은
	<%=str%>
---------------연동---------------------
    <form method="post" action="hi.jsp">
		이름 : <input name="name">
		<p />
		좋아하는 음식 : <select name="food">
			<option value="ramen" selected>라면</option>
			<option value="pizza">피자</option>
			<option value="chick">치킨</option>
		</select> <input type="submit" value="제출">
	</form>

    ----------------------------




